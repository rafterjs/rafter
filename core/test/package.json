{
  "name": "@rafterjs/test",
  "description": "Testing utils and helpers",
  "version": "0.8.8",
  "contributors": [
    {
      "name": "Josh Stuart",
      "email": "joshstuartx@gmail.com"
    }
  ],
  "license": "GPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:lib && npm run build:index && npm run build:types",
    "build:lib": "babel ./lib --out-dir dist/lib --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:index": "babel index.ts --out-dir dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf ./dist",
    "lint": "eslint \"./lib/**/*\" ./babel.config.js",
    "lint:fix": "yarn lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafterjs/rafter.git",
    "directory": "core/test"
  },
  "homepage": "https://github.com/rafterjs/rafter/tree/master/core/test",
  "dependencies": {
    "node-fetch": "2.6.1",
    "stubby": "5.1.0",
    "ts-sinon": "2.0.1"
  },
  "devDependencies": {
    "@rafterjs/babel-preset-rafter": "^0.8.8",
    "@rafterjs/eslint-config": "^0.8.8",
    "@rafterjs/logger-plugin": "^0.8.8",
    "@types/node-fetch": "2.5.8"
  },
  "engines": {
    "node": ">=14"
  },
  "gitHead": "11da44fed33e1fbddfbb7bab5ca88116a58fcb70"
}
